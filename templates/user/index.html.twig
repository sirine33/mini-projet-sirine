{% extends 'base.html.twig' %}

{% block title %}List Produit{% endblock %}

{% block body %}

<h1>List Of User</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">User</th>
      <th scope="col">Email</th>
    
    
    </tr>
  </thead>
  <tbody>
  {% for product in products %}
    <tr>
      
      <th scope="row">{{product.Lastname}}</th>
      
      
      <td>{{product.email}}</td>
      <td><a href="{{path('detail1',{'id':product.id})}}" class="btn btn-primary">details</a>
      </td> 
    </tr>
    {% endfor %}
  </tbody>
</table>
{% endblock %}
